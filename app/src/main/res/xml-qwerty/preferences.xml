<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
	<CheckBoxPreference
		android:key="vibration_setting"

		android:defaultValue="true"
		/>
	<RingtonePreference
		android:key="ringtone_setting"

		android:showSilent="true"
		android:showDefault="true"
		android:ringtoneType="ringtone|alarm"
		/>
	<CheckBoxPreference
		android:key="snap_setting"

		android:defaultValue="true"
		/>
	<CheckBoxPreference
		android:key="ramping_setting"
		android:defaultValue="true"
		/>
	<ListPreference
		android:key="theme_setting"

		android:defaultValue="0"

		/>
</PreferenceScreen>
